<?xml version="1.0" encoding="UTF-8"?>
<eventBuilder name="jmsgeodatabuilder" statistics="disable"
    trace="enable" xmlns="http://wso2.org/carbon/eventbuilder">
    <from eventAdaptorName="jmsQueueAdaptor" eventAdaptorType="jms">
        <property name="transport.jms.Destination">geodata</property>
    </from>
    <mapping customMapping="enable" type="xml">
        <xpathDefinition namespace="http://samples.wso2.org/" prefix="geodata"/>
        <property>
            <from xpath="//geodata:data/geodata:id"/>
            <to default="0.0" name="id" type="string"/>
        </property>
        <property>
            <from xpath="//geodata:data/geodata:timeStamp"/>
            <to name="timeStamp" type="string"/>
        </property>
        <property>
            <from xpath="//geodata:data/geodata:latitude"/>
            <to name="latitude" type="double"/>
        </property>
        <property>
            <from xpath="//geodata:data/geodata:longitude"/>
            <to name="longitude" type="double"/>
        </property>
        <property>
            <from xpath="//geodata:data/geodata:speed"/>
            <to default="0.0" name="speed" type="float"/>
        </property>
        <property>
            <from xpath="//geodata:data/geodata:angle"/>
            <to default="0.0" name="heading" type="float"/>
        </property>
    </mapping>
    <to streamName="rawInputStream" version="1.0.0"/>
</eventBuilder>
